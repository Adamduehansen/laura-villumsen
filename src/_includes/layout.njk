---
webtitle: Laura Villumsen, Grafisk designer
socials:
  [
    { name: instragram, link: 'https://instagram.com' },
    { name: linkedin, link: 'https://linkedin.com' },
  ]
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link rel="stylesheet" href="/assets/tailwind.css">
    <title></title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Syne&display=swap" rel="stylesheet">
    <script defer src="assets/main.js"></script>
  </head>
  <body class="font-syne">
    <header class="fixed inset-x-0 bg-green py-4 px-3">
      <nav>
        <a href="/">{{ webtitle }}</a>
      </nav>
    </header>
    <main class="pt-16">{{ content | safe }}</main>
    <footer class="fixed bottom-0 px-3 w-full text-right">
      <span class="mr-10">footerâ†’</span>
      {% for social in socials %}
        <a href="{{ social.link }}" class="underline mr-6 last:mr-0">{{ social.name }}</a>
      {% endfor %}
    </footer>
  </body>
</html>